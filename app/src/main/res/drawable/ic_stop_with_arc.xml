<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="960"
    android:viewportHeight="960">

    <!-- Stationary square -->
    <path
        android:pathData="M320,640h320v-320L320,320v320Z"
        android:fillColor="#54808C"/>

    <!-- Rotating arc group -->
    <group
        android:name="arc_rotation_group"
        android:pivotX="480"
        android:pivotY="480">

        <!-- Arc path (1/3 of the original circle, adjusted to correct radius ~360 for the ring) -->
        <path
            android:name="arc_path"
            android:pathData="M840,480 A360,360 0 0,1 300,791.77"
            android:strokeWidth="80"
            android:strokeColor="#54808C"
            android:fillColor="@android:color/transparent"/>
    </group>
</vector>
